{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*",
  "PostgresConfig": {
    "user": "PostgreSQL:user",
    "password": "PostgreSQL:password",
    "hostname": "PostgreSQL:hostname",
    "port": "PostgreSQL:port",
    "database": "PostgreSQL:database"
  },
  "JwtConfig": {
    "Secret": "JwtConfig:Secret",
    "Expires": "JwtConfig:Expires"
  },
  "SmtpConfig": {
    "Address": "SmtpConfig:Address",
    "Password": "SmtpConfig:Password",
    "Host": "SmtpConfig:Domain",
    "Port": "SmtpConfig:Port"
  },
  "SwaggerConfig": {
    "EndpointUrl": "/swagger/v1/swagger.json",
    "EndpointName": "inventart-api v1"
  },
  "GlobalConfig": {
    "VerificationPrefix": "GlobalConfig:VerificationPrefix"
  }
}
